<h2>Listing Events</h2>

<table class="table">
  <tbody>
<%= for event <- @events do %>
    <tr>
      <td><a href="<%= event.link %>"><%= event.name %></a></td>
      <td><%= event.date %></td>
      <td><%= event.description %></td>
      <td><%= event.rsvp %> foinf</td>
    </tr>
<% end %>
  </tbody>
</table>

<span><%= link "New Event", to: event_path(@conn, :new) %></span>
